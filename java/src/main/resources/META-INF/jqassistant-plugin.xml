<?xml version="1.0" encoding="UTF-8"?>
<jqassistant-plugin
        xmlns="http://schema.jqassistant.org/plugin/v2.4"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://schema.jqassistant.org/plugin/v2.4
                            https://schema.jqassistant.org/plugin/jqassistant-plugin-v2.4.xsd"
        id="jqassistant.plugin.csharp"
        name="jQAssistant CSharp Plugin"
        version="${project.version}">

    <description>
        jQAssistant plugin for analyzing C# projects using Roslyn and mapping
        the structure into the jQAssistant graph model.
    </description>

    <!-- Domain Model Definitions (Descriptors / Labels) -->
    <model>
        <!-- Project -->
        <class>org.jqassistant.plugin.csharp.api.descriptors.CSharpClassDescriptor</class>
        <class>org.jqassistant.plugin.csharp.api.descriptors.CSharpEnumDescriptor</class>
        <class>org.jqassistant.plugin.csharp.api.descriptors.CSharpFieldDescriptor</class>
        <class>org.jqassistant.plugin.csharp.api.descriptors.CSharpInterfaceDescriptor</class>
        <class>org.jqassistant.plugin.csharp.api.descriptors.CSharpMethodDescriptor</class>
        <class>org.jqassistant.plugin.csharp.api.descriptors.CSharpNamespaceDescriptor</class>
        <class>org.jqassistant.plugin.csharp.api.descriptors.CSharpParameterDescriptor</class>
        <class>org.jqassistant.plugin.csharp.api.descriptors.CSharpProjectDescriptor</class>
        <class>org.jqassistant.plugin.csharp.api.descriptors.CSharpPropertyDescriptor</class>
        <class>org.jqassistant.plugin.csharp.api.descriptors.CSharpRecordDescriptor</class>
        <class>org.jqassistant.plugin.csharp.api.descriptors.CSharpStructDescriptor</class>
        <class>org.jqassistant.plugin.csharp.api.descriptors.CSharpTypeDescriptor</class>

    </model>

    <!-- Optional: A custom scope, if the plugin defines one -->
    <scope>
        <class>org.jqassistant.plugin.csharp.api.CSharpScope</class>
    </scope>

    <!-- Scanner Plugin Implementation -->
    <scanner>
        <class>org.jqassistant.plugin.csharp.impl.scanner.CSharpProjectScannerPlugin</class>
    </scanner>

</jqassistant-plugin>
